<?xml version="1.0"?>
<robot name="open_frame_claw_machine">

  <material name="dark_grey">
    <color rgba="0.2 0.2 0.2 1" />
  </material>
  <material name="red">
    <color rgba="0.8 0.1 0.1 1" />
  </material>
  <material name="green">
    <color rgba="0.1 0.8 0.1 1" />
  </material>
  <material name="blue">
    <color rgba="0.1 0.1 0.8 1" />
  </material>
  <material name="white">
    <color rgba="0.9 0.9 0.9 1" />
  </material>

  <link name="world" />

  <joint name="fixed_base" type="fixed">
    <parent link="world" />
    <child link="frame_structure" />
  </joint>

  <link name="frame_structure">
    <inertial>
      <mass value="200.0" />
      <origin xyz="0 0 0.75" />
      <inertia ixx="50.0" ixy="0.0" ixz="0.0" iyy="50.0" iyz="0.0" izz="50.0" />
    </inertial>

    <visual>
      <origin xyz="1.0 1.0 0.75" rpy="0 0 0" />
      <geometry>
        <box size="0.1 0.1 1.5" />
      </geometry>
      <material name="dark_grey" />
    </visual>
    <collision>
      <origin xyz="1.0 1.0 0.75" rpy="0 0 0" />
      <geometry>
        <box size="0.1 0.1 1.5" />
      </geometry>
    </collision>

    <visual>
      <origin xyz="1.0 -1.0 0.75" rpy="0 0 0" />
      <geometry>
        <box size="0.1 0.1 1.5" />
      </geometry>
      <material name="dark_grey" />
    </visual>
    <collision>
      <origin xyz="1.0 -1.0 0.75" rpy="0 0 0" />
      <geometry>
        <box size="0.1 0.1 1.5" />
      </geometry>
    </collision>

    <visual>
      <origin xyz="-1.0 1.0 0.75" rpy="0 0 0" />
      <geometry>
        <box size="0.1 0.1 1.5" />
      </geometry>
      <material name="dark_grey" />
    </visual>
    <collision>
      <origin xyz="-1.0 1.0 0.75" rpy="0 0 0" />
      <geometry>
        <box size="0.1 0.1 1.5" />
      </geometry>
    </collision>

    <visual>
      <origin xyz="-1.0 -1.0 0.75" rpy="0 0 0" />
      <geometry>
        <box size="0.1 0.1 1.5" />
      </geometry>
      <material name="dark_grey" />
    </visual>
    <collision>
      <origin xyz="-1.0 -1.0 0.75" rpy="0 0 0" />
      <geometry>
        <box size="0.1 0.1 1.5" />
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 1.0 1.5" rpy="0 0 0" />
      <geometry>
        <box size="2.1 0.1 0.1" />
      </geometry>
      <material name="dark_grey" />
    </visual>
    <collision>
      <origin xyz="0 1.0 1.5" rpy="0 0 0" />
      <geometry>
        <box size="2.1 0.1 0.1" />
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 -1.0 1.5" rpy="0 0 0" />
      <geometry>
        <box size="2.1 0.1 0.1" />
      </geometry>
      <material name="dark_grey" />
    </visual>
    <collision>
      <origin xyz="0 -1.0 1.5" rpy="0 0 0" />
      <geometry>
        <box size="2.1 0.1 0.1" />
      </geometry>
    </collision>

    <visual>
      <origin xyz="1.0 0 1.5" rpy="0 0 0" />
      <geometry>
        <box size="0.1 2.0 0.1" />
      </geometry>
      <material name="dark_grey" />
    </visual>
    <collision>
      <origin xyz="1.0 0 1.5" rpy="0 0 0" />
      <geometry>
        <box size="0.1 2.0 0.1" />
      </geometry>
    </collision>

    <visual>
      <origin xyz="-1.0 0 1.5" rpy="0 0 0" />
      <geometry>
        <box size="0.1 2.0 0.1" />
      </geometry>
      <material name="dark_grey" />
    </visual>
    <collision>
      <origin xyz="-1.0 0 1.5" rpy="0 0 0" />
      <geometry>
        <box size="0.1 2.0 0.1" />
      </geometry>
    </collision>
  </link>

  <joint name="joint_x" type="prismatic">
    <parent link="frame_structure" />
    <child link="x_bridge" />
    <origin xyz="0 0 1.55" rpy="0 0 0" />
    <axis xyz="1 0 0" />
    <limit lower="-0.85" upper="0.85" effort="100" velocity="1.0" />
  </joint>

  <link name="x_bridge">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.15 2.2 0.05" />
      </geometry>
      <material name="red" />
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.15 2.2 0.05" />
      </geometry>
    </collision>
    <inertial>
      <mass value="5.0" />
      <inertia ixx="0.2" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.2" />
    </inertial>
  </link>

  <joint name="joint_y" type="prismatic">
    <parent link="x_bridge" />
    <child link="y_carriage" />
    <origin xyz="0 0 0.05" rpy="0 0 0" />
    <axis xyz="0 1 0" />
    <limit lower="-0.9" upper="0.9" effort="100" velocity="1.0" />
  </joint>

  <link name="y_carriage">
    <visual>
      <geometry>
        <box size="0.15 0.15 0.05" />
      </geometry>
      <material name="green" />
    </visual>
    <collision>
      <geometry>
        <box size="0.15 0.15 0.05" />
      </geometry>
    </collision>
    <inertial>
      <mass value="2.0" />
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01" />
    </inertial>
  </link>

  <joint name="joint_z" type="prismatic">
    <parent link="y_carriage" />
    <child link="z_plunger" />
    <origin xyz="0 0 0.8" rpy="0 0 0" />
    <axis xyz="0 0 -1" />
    <limit lower="0.0" upper="1.10" effort="100" velocity="1.0" />
  </joint>

  <link name="z_plunger">
    <visual>
      <origin xyz="0 0 -0.3" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.04" length="1.6" />
      </geometry>
      <material name="blue" />
    </visual>
    <collision>
      <origin xyz="0 0 -0.3" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.04" length="1.6" />
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0" />
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.01" />
    </inertial>
  </link>

  <joint name="plunger_to_palm" type="fixed">
    <parent link="z_plunger" />
    <child link="gripper_palm" />
    <origin xyz="0 0 -1.1" rpy="0 0 0" />
  </joint>

  <link name="gripper_palm">
    <visual>
      <geometry>
        <box size="0.25 0.05 0.05" />
      </geometry>
      <material name="white" />
    </visual>
    <collision>
      <geometry>
        <box size="0.25 0.05 0.05" />
      </geometry>
    </collision>
    <inertial>
      <mass value="0.25" />
      <inertia ixx="0.002" ixy="0.0" ixz="0.0" iyy="0.002" iyz="0.0" izz="0.002" />
    </inertial>
  </link>

  <joint name="joint_gripper" type="prismatic">
    <parent link="gripper_palm" />
    <child link="right_finger" />
    <origin xyz="0.115 0 0" rpy="0 0 0" />
    <axis xyz="-1 0 0" />
    <limit lower="0.0" upper="0.105" effort="20" velocity="0.5" />
  </joint>

  <link name="right_finger">
    <visual>
      <origin xyz="0 0 -0.1" rpy="0 0 0" />
      <geometry>
        <box size="0.02 0.05 0.2" />
      </geometry>
      <material name="white" />
    </visual>
    <collision>
      <origin xyz="0 0 -0.1" rpy="0 0 0" />
      <geometry>
        <box size="0.02 0.05 0.2" />
      </geometry>
    </collision>
    <inertial>
      <mass value="0.05" />
      <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001" />
    </inertial>
  </link>

  <joint name="fixed_left_finger" type="fixed">
    <parent link="gripper_palm" />
    <child link="left_finger" />
    <origin xyz="-0.115 0 0" rpy="0 0 0" />
  </joint>

  <link name="left_finger">
    <visual>
      <origin xyz="0 0 -0.1" rpy="0 0 0" />
      <geometry>
        <box size="0.02 0.05 0.2" />
      </geometry>
      <material name="white" />
    </visual>
    <collision>
      <origin xyz="0 0 -0.1" rpy="0 0 0" />
      <geometry>
        <box size="0.02 0.05 0.2" />
      </geometry>
    </collision>
    <inertial>
      <mass value="0.05" />
      <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001" />
    </inertial>
  </link>

</robot>